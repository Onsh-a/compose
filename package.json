{
  "name": "compose",
  "version": "1.0.0",
  "description": "app to compose",
  "type": "module",
  "targets": {
    "web": {
      "context": "browser"
    }
  },
  "scripts": {
    "watch": "parcel serve index.html --port 3000",
    "watch_open": "parcel serve index.html --open --port 3000",
    "build": "parcel build index.html",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "prepare_deploy": "rm -rf dist/ && rm -rf node_modules/.cache/gh-pages && npm run build",
    "deploy": "npm run prepare_deploy && gh-pages -d dist"
  },
  "homepage": "https://onsh-a.github.io/compose",
  "author": "Onsha",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/register": "^7.18.9",
    "@parcel/transformer-sass": "^2.6.1",
    "@parcel/transformer-vue": "^2.6.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "sass": "^1.51.0"
  },
  "dependencies": {
    "gh-pages": "^4.0.0",
    "helpers": "^0.0.6",
    "parcel": "^2.6.1",
    "pinia": "^2.0.28",
    "src": "^1.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  }
}
